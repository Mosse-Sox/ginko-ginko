      "settings": {
        "foreground": "#cb2769"
      }
    },
    {
      "scope": ["source.scss keyword.control"],
      "settings": {
        "foreground": "#22bd31"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#547fdc"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#b4236b"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#c85f26"
      }
